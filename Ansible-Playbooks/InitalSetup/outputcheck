---
 - name: check output 
   become: yes 
   hosts: all 
   tasks:
     - name: Get the list of users in group functids to be added to sshd  file
       shell: /usr/bin/lsgroup -a users functids | /usr/bin/awk -F= '{print $2}'
       register: functids_users
     - name: debug
       shell:  "echo {{ functids_users.stdout_lines[0] }} >> /tmp/otput.txt"
